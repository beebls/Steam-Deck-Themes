{
  "name": "Colored Compatibility Icons",
  "author": "Crush & Beebles",
  "version": "v1.1",
  "target": "Library",
  "inject": {
    "shared.css": ["SP"]
  },
  "manifest_version": 3,
  "patches": {
    "Deck Icon Color": {
      "default": "Standard",
      "type": "dropdown",
      "values": {
        "Standard": {
          "deckIcon/standard.css": ["SP"]
        },
        "Custom": {},
        "Pink": {
          "deckIcon/pink.css": ["SP"]
        },
        "Black": {
          "deckIcon/black.css": ["SP"]
        },
        "Green": {
          "deckIcon/green.css": ["SP"]
        },
        "Navy": {
          "deckIcon/navy.css": ["SP"]
        },
        "Orange": {
          "deckIcon/orange.css": ["SP"]
        },
        "Purple": {
          "deckIcon/purple.css": ["SP"]
        },
        "Red": {
          "deckIcon/red.css": ["SP"]
        },
        "Teal": {
          "deckIcon/teal.css": ["SP"]
        },
        "Yellow": {
          "deckIcon/yellow.css": ["SP"]
        }
      },
      "components": [
        {
          "name": "Deck Icon",
          "type": "color-picker",
          "on": "Custom",
          "default": "#dcdedf",
          "css_variable": "custom-compat-icons-deck",
          "tabs": ["SP"]
        }
      ]
    },
